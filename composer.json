{
  "name": "job-runner/symfony-notifier-adapter",
  "homepage": "https://github.com/job-runner/symfony-notifier-adapter",
  "license": "MIT",
  "description": "",
  "authors": [
    {
      "name": "St√©phane Demonchaux",
      "email": "demonchaux.stephane@gmail.com",
      "homepage": "https://github.com/fezfez"
    }
  ],
  "require": {
    "php": "~8.2.0 || ~8.3.0 || ~8.4.0",
    "job-runner/job-runner": "^1.4",
    "symfony/notifier": "^6.0 || ^7.0"
  },
  "require-dev": {
    "doctrine/coding-standard": "^12.0",
    "phpunit/phpunit": "^11.5.3",
    "phpstan/phpstan": "^2.1.1"
  },
  "autoload": {
    "psr-4": {
      "JobRunner\\JobRunner\\SymfonyNotifier\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JobRunner\\JobRunner\\SymfonyNotifier\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true
    },
    "platform": {
      "php": "8.2.99"
    }
  }
}
