  "require": {
    "php": "~8.3.0 || ~8.4.0 || ~8.5.0",
    "job-runner/job-runner": "^1.6",
    "symfony/notifier": "^6.0 || ^7.0 || ^8.0"
  },
  "require-dev": {
    "doctrine/coding-standard": "^14.0",
    "phpstan/phpstan": "^2.1.34",
    "phpunit/phpunit": "^12.5.5"
  },
  "autoload": {
    "psr-4": {
      "JobRunner\\JobRunner\\SymfonyNotifier\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JobRunner\\JobRunner\\SymfonyNotifier\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true
    },
    "platform": {
      "php": "8.3.99"
    }
  }
}
